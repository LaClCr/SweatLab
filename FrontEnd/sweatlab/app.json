{
  "expo": {
    "name": "sweatlab",
    "slug": "sweatlab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logos/SweatLab_Logo_APP.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./src/assets/logos/SweatLab_Logo_APP.png",
      "resizeMode": "contain",
      "backgroundColor": "#391059"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.laiaclcr.sweatlab"
    },
    "android": {
      "package": "com.laiaclcr.sweatlab",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logos/SweatLab_Logo_APP.png",
        "backgroundColor": "#391059"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2d5879fd-3658-4e4e-a204-b4e2ce491368"
      }
    }
  }
}
